#!/bin/bash

# Build script for development mode
# This serves as a replacement for npm run build:dev

echo "🚀 Running development build..."

# Use the existing build:debug script which does exactly what build:dev should do
npm run build:debug

if [ $? -eq 0 ]; then
    echo "✅ Development build completed successfully!"
else
    echo "❌ Development build failed!"
    exit 1
fi